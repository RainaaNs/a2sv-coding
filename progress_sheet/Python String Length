def print_full_name(first, last):
    first = len(first_name)
    last = len(last_name)
    if first <= 10 and last <= 10:
        print(f"Hello {first_name} {last_name}! You just delved into python.")
    else:
        print(">10")

if __name__ == '__main__':
    first_name = input()
    last_name = input()
